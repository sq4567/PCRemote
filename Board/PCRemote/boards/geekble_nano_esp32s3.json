{
  "name": "Geekble nano ESP32-S3 (ESP32-S3FH4R2)",
  "url": "https://github.com/SooDragon/Geekble-nano-ESP32S3",
  "vendor": "Geekble",
  "mcu": "esp32s3",
  "connectivity": ["wifi", "bluetooth", "usb"],
  "frameworks": ["arduino"],
  "platforms": ["espressif32"],
  "build": {
    "core": "esp32",
    "mcu": "esp32s3",
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "psram": "qspi",
    "arduino": {
      "ldscript": "esp32s3_out.ld",
      "memory_type": "qio_qspi"
    },
    "extra_flags": "-DARDUINO_ESP32S3_DEV -DARDUINO_USB_MODE=1 -DARDUINO_USB_CDC_ON_BOOT=1 -DBOARD_HAS_PSRAM",
    "variant": "esp32s3"
  },
  "upload": {
    "maximum_size": 4194304,
    "flash_size": "4MB",
    "maximum_ram_size": 524288,
    "speed": 921600,
    "require_upload_port": true
  },
  "debug": {
    "openocd_target": "esp32s3.cfg"
  }
}


